{
    "manifest_version": 2,
    "name": "Taskru",
    "description": "get notification from Taskru",
    "version": "1.0",
    "browser_action": {
        "default_icon": "images/taskru_logo.png",
        "default_title": "Taskru Notification"
    },
    "background": {
        "scripts": ["js/jquery-2.1.4.min.js","js/background.js"],
        "persistent": false
    },
    "options_page": "option.html",
    "permissions": [
        "gcm",
        "background",
        "notifications",
        "storage"
    ]
}