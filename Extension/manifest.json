{
    "manifest_version": 2,
    "name": "Taskru",
    "description": "get notification from Taskru",
    "version": "1.0",
    "browser_action": {
        "degault_icon": {
            "default_icon": "images/taskru_logo.png"
        },
        "default_title": "Taskru Notification"
    },
    "background": {
        "scripts": ["jquery-2.1.4.min.js","background.js"],
        "persistent": false
    },
    "permissions": [
        "gcm",
        "background",
        "notifications",
        "storage"
    ]
}