{
    "manifest_version": 2,
    "name": "Taskru",
    "description": "get notification from Taskru",
    "version": "1.0",
    "browser_action": {
        "degault_icon": {
            "19": "images/icon/icon_disable.png",
            "38": "images/icon/icon_disable.png"
        },
        "default_title": "Taskru Notification",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["js/jquery-2.1.4.min.js", "js/background.js"],
        "persistent": false
    },
    "permissions": [
        "tabs",
        "storage"
    ]
}